#Create object from prototype
newsPage = Sfi.Sfi:Page
newsPage.body.content.main = Sfi.News:Detail
#Route the rendering to our newsPage object
root.newsPage {
	condition = ${q(node).is('[instanceof Sfi.News:News]')}
	renderPath = '/newsPage'
}

categoryPage = Sfi.Sfi:Page
categoryPage.body.content.main>
root.categoryPage {
	condition = ${q(node).is('[instanceof Sfi.News:Category]')}
	renderPath = '/categoryPage'
}